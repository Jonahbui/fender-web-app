<body>
  <!-- 
    Cannot pass components as props to react_component. Really slow as a result.
    All .jsx/.js files get recompiled into a single file and delivered.
  --->
  <%= 
      tickets_content = [
        {
          key: "queue-service-fender-connection",
          link: "",
          link_text: "Fender Connection",
        },
        {
          key: "queue-service-current-ticket",
          link: "",
          link_text: "Current Ticket",
        },
        {
          key: "queue-service-total-tickets",
          link: "",
          link_text: "Total Tickets",
        }
      ]

    steps = [
      {
        label: 'Ticket Received',
        description: 'Request is being processed'
      },
      {
        label: 'Arriving at Departure location',
        description:
          'The Fender is on its way to the departure location'
      },
      {
        label: 'Load Package',
        description: 'Place package into the Fender and confirm'
      },
      {
        label: 'Arriving at Destination',
        description: 'The Fender is on its way to the destination'
      },
      {
        label: 'Arrived',
        description: 'Ready for pickup at destination'
      }
    ]
      info_content = [
      {
        key: "queue-info-fender-connection",
        primary_text: "Fender Connection",
        secondary_text: Bot.instance.connected?
      },
      {
        key: "queue-info-current-ticket",
        primary_text: "Current Ticket",
        secondary_text: "1"
      },
      {
        key: "queue-info-total-tickets",
        primary_text: "Total Tickets",
        secondary_text: "1"
      }
    ]
    react_component("queue/Index", {steps: steps, tickets_content: tickets_content, info_content: info_content}) 
  %>
</body>